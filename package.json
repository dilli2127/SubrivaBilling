{
  "name": "FOCUZ-BILLING",
  "version": "0.1.0",
  "description": "Professional billing and inventory management desktop application",
  "author": "Fresh Focuz Tech",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.27",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "antd": "^5.26.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.15.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.3",
    "redux": "^5.0.1",
    "swiper": "^11.2.8",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.2",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "archiver": "^7.0.1",
    "async-mutex": "^0.5.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-mung": "^0.5.1",
    "express-validator": "^7.2.1",
    "greenlock": "^2.8.9",
    "helmet": "^7.1.0",
    "inquirer": "^12.6.3",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.11.0",
    "node-cron": "^3.0.3",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "punycode": "^2.3.1",
    "request": "^2.88.2",
    "request-ip": "^3.3.0",
    "request-promise": "^4.2.6",
    "sequelize": "^6.37.3",
    "sharp": "^0.33.5",
    "spdy": "^4.0.2",
    "umzug": "3"
  },
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "dev": "concurrently \"npm run start\" \"npm run lint:fix\"",
    "electron": "npx electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && npx electron .\"",
    "electron-full-dev": "concurrently \"npm start\" \"npm run backend-dev\" \"wait-on http://localhost:3000 && npx electron .\"",
    "electron-pack": "npm run build && npx electron-builder",
    "preelectron-pack": "npm run build",
    "build-electron": "npm run build && npm run install-backend-deps && npx electron-builder --publish=never",
    "dist": "npm run build && npm run install-backend-deps && npx electron-builder --publish=never",
    "install-backend-deps": "cd backend && npm install --production",
    "postinstall": "cd backend && npm install --production",
    "backend": "cd backend && node src/server.js",
    "backend-dev": "cd backend && nodemon src/server.js",
    "dev-full": "concurrently \"npm start\" \"npm run backend-dev\"",
    "electron-dev-full": "concurrently \"npm start\" \"npm run backend-dev\" \"wait-on http://localhost:3000 && electron .\"",
    "setup": "npm install && npm run install-backend-deps",
    "clean": "rimraf build dist node_modules backend/node_modules",
    "reset": "npm run clean && npm run setup"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/leaflet": "^1.9.15",
    "@types/react-color": "^3.0.13",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "32.0.0",
    "electron-builder": "^25.0.0",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "wait-on": "^8.0.3",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "rimraf": "^5.0.5"
  },
  "build": {
    "appId": "com.freshfocuztech.probilldesk",
    "productName": "SubrivaBilling",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "backend-server.js",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "backend",
        "to": "backend"
      }
    ],
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "extraMetadata": {
      "main": "main.js"
    },
    "mac": {
      "category": "public.app-category.business",
      "icon": "src/assets/img/icon.jpg"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/img/icon.jpg"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/img/icon.jpg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": {
      "provider": "github",
      "owner": "your-username",
      "repo": "SubrivaBilling"
    }
  }
}
