{
  "name": "SUBRIVA-BILLING",
  "version": "2.0.2",
  "description": "Professional billing and inventory management desktop application",
  "author": "Fresh Focuz Tech",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.15.27",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-window": "^1.8.8",
    "antd": "^5.26.0",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.7",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.15.0",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.3",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "react-window": "^2.2.1",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^2.15.3",
    "redux": "^5.0.1",
    "swiper": "^11.2.8",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.2",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
  },
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "dev": "concurrently \"npm run start\" \"npm run lint:fix\"",
    "electron": "npx electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && npx electron .\"",
    "electron-pack": "npm run build && npx electron-builder",
    "preelectron-pack": "npm run build",
    "build-electron": "npm run build && npx electron-builder --publish=never",
    "dist": "npm run build && npx electron-builder --publish=never",
    "clean": "rimraf build dist node_modules",
    "reset": "npm run clean && npm install",
    "version:patch": "node scripts/update-version.js patch",
    "version:minor": "node scripts/update-version.js minor",
    "version:major": "node scripts/update-version.js major",
    "version:patch:publish": "node scripts/update-version.js patch --publish",
    "version:minor:publish": "node scripts/update-version.js minor --publish",
    "version:major:publish": "node scripts/update-version.js major --publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/html2canvas": "^0.5.35",
    "@types/leaflet": "^1.9.15",
    "@types/react-color": "^3.0.13",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "32.0.0",
    "electron-builder": "^25.0.0",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.freshfocuztech.probilldesk",
    "productName": "SubrivaBilling",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "node_modules/**/*"
    ],
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "extraMetadata": {
      "main": "main.js"
    },
    "mac": {
      "category": "public.app-category.business",
      "icon": "src/assets/img/icon.jpg"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/img/icon.jpg"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/img/icon.jpg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": {
      "provider": "github",
      "owner": "dilli2127",
      "repo": "SubrivaBilling",
      "releaseType": "release"
    }
  }
}
