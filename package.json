    "@types/node": "^22.15.27",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "antd": "^5.26.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.15.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "^0.0.0",
